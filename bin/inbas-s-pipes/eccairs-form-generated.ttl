  rdfs:label "Pilot response to RA"^^xsd:string ;
.
form-ecc:a-613
  rdf:type form:question-template ;
  rdf:type owl:NamedIndividual ;
  form:origin <http://onto.fel.cvut.cz/ontologies/eccairs-3.4.0.2/a-613> ;
  rdfs:comment "Resolution advisory classification: the classification of an indication given to the flight crew recommending a manoeuvre intended to provide separation from all threats; or a manoeuvre restriction intended to maintain existing separation."^^xsd:string ;
  rdfs:label "RA Classification"^^xsd:string ;
.
form-ecc:a-619
